{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "#file",
  "title": "Web Api javascript file",
  "type": "object",
  "additionalProperties": true,
  "required": ["mimetype", "originalname", "size"],
  "properties": {
    "mimetype": {
      "type": "string",
      "pattern": "^[-\\w.]+/[-\\w.]+$"
    },
    "originalname": {
      "type": "string"
    },
    "encoding": {
      "type": "string"
    },
    "size": {
      "type": "number"
    }
  }
}
