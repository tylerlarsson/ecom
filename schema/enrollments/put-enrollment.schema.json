{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "put enrollment configuration schema",
  "type": "object",
  "required": ["body", "params"],
  "additionalProperties": false,
  "properties": {
    "body": {
      "type": "object",
      "required": ["step"],
      "additionalProperties": false,
      "properties": {
        "step": {
          "$ref": "#mongo-id"
        }
      }
    },
    "params": {
      "type": "object",
      "required": ["enrollment"],
      "additionalProperties": false,
      "properties": {
        "enrollment": {
          "$ref": "#mongo-id"
        }
      }
    }
  }
}
